<ui:UiBinder
    xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with type="com.example.jumpnote.web.client.resources.Resources" field="res" />
    <g:HTMLPanel styleName="noteItem">
        <g:Label styleName="noteTitle" ui:field="titleLabel"/>
        <g:Label styleName="noteSnippet" ui:field="snippetLabel"/>
        <div class="controls">
            <g:PushButton ui:field="editButton">
                <g:upFace image="{res.btn_edit}"/>
                <g:upHoveringFace image="{res.btn_edit_on}"/>
                <g:downFace image="{res.btn_edit_on}"/>
            </g:PushButton>
            <g:PushButton ui:field="deleteButton">
                <g:upFace image="{res.btn_delete}"/>
                <g:upHoveringFace image="{res.btn_delete_on}"/>
                <g:downFace image="{res.btn_delete_on}"/>
            </g:PushButton>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
